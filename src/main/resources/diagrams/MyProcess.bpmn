        <omgdi:waypoint x="550.0" y="154.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow4" id="BPMNEdge_flow4">
        <omgdi:waypoint x="165.0" y="154.0"></omgdi:waypoint>
        <omgdi:waypoint x="242.0" y="154.0"></omgdi:waypoint>
